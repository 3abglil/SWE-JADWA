    <!-- ============= Home Section =============== -->
    <section class="home">
      <div class="container">
        <div class="right">
          <h2 id="j">Add User</h2>
          <form action="/admin/adduser" method="post">
            <input type="text"     name="username" class="field" placeholder="User's UserName" />
            <input type="text"     name="Email"    class="field" placeholder="User's Email" />
            <input type="password" name="password" class="field" placeholder="User's Password" />
            <input type="text"     name="phone"    class="field" placeholder="User's phone" />
            <input type="text"     name="address"  class="field" placeholder="User's address" />
            <input type="hidden"   name="type" value="client">
            <select class="field"  name="gender">
              <option value="">Gender</option>
              <option value="male">Male</option>
              <option value="female">Female</option>
          </select>
          <input type="submit" class="btn" value="ADD">
          </form>
        

        
          
        </div>
